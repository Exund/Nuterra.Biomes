{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"name": {
			"type": "string",
			"description": "Biome group name"
		},
		"m_Biomes": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"minItems": 1,
			"description": "Biomes names"
		},
		"m_BiomeWeights": {
			"type": "array",
			"items": {
				"type": "number"
			},
			"minItems": 1,
			"description": "Biomes weights"
		},
		"m_WeightingByDistance": {
			"$ref": "./ExtendedAnimationCurve.schema.json",
			"description": "Weighting by distance"
		}
	},
	"required": [
		"name",
		"m_Biomes",
		"m_BiomeWeights",
		"m_WeightingByDistance"
	]
}